ination-button\");\r\n\r\n      if (!btn) return;\r\n\r\n      const goToPage = +btn.dataset.goto;\r\n\r\n      handler(goToPage);\r\n    });\r\n  }\r\n\r\n  render(data) {\r\n    this._clear();\r\n    this.#data = data;\r\n    const markup = this.#generateMarkup(data);\r\n    this.#parentEl.insertAdjacentHTML(\"afterbegin\", markup);\r\n  }\r\n\r\n  #generateMarkup(data) {\r\n    const curPage = data.page;\r\n    const numPages = Math.ceil(data.results.length / data.resultsPerPage);\r\n    //   Page 1 and other pages\r\n    if (curPage === 1 && numPages > 1) {\r\n      return `\r\n      \r\n      <button data-goto='${\r\n        curPage + 1\r\n      }' class=\"pagination-button btn-next\" id=\"pagination\">Page ${\r\n        curPage + 1\r\n      }</button>\r\n  \r\n    `;\r\n    }\r\n\r\n    // Last Page\r\n    if (curPage === numPages && numPages > 1) {\r\n      return `\r\n      \r\n      <button data-goto='${\r\n        curPage - 1\r\n      }' class=\"pagination-button btn-previous\" id=\"pagination\">Page ${\r\n        curPage - 1\r\n      }</button>\r\n  \r\n    `;\r\n    }\r\n\r\n    // Other Page\r\n    if (curPage < numPages) {\r\n      return `\r\n      \r\n      <button data-goto='${\r\n        curPage - 1\r\n      }' class=\"pagination-button btn-next\" id=\"pagination\">Page ${\r\n        curPage - 1\r\n      }</button>\r\n      <button data-goto='${\r\n        curPage + 1\r\n      }' class=\"pagination-button btn-next\" id=\"pagination\">Page ${\r\n        curPage + 1\r\n      }</button>\r\n  \r\n    `;\r\n    }\r\n\r\n    // Page 1 and NO other pages\r\n    return \"\";\r\n  }\r\n}\r\n\r\nexport default new PaginationView();\r\n"},"lineCount":null}},"error":null,"hash":"43c6c63d4514924b6b4efcd105b532ae","cacheData":{"env":{}}}